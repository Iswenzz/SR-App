{
 "name": "speedrunclient",
 "version": "1.0.0",
 "description": "",
 "main": "src/main.js",
 "type": "module",
 "scripts": {
  "start": "electron .",
  "dev": "set NODE_ENV=debug&&electron .",
  "test": "jest --watchAll=false --coverage --coverageDirectory=coverage",
  "package-mac": "electron-packager . speedrun-app --overwrite --platform=darwin --arch=x64 --icon=views/assets/icons/mac/icon.icns --prune=true --out=release-builds --darwinDarkModeSupport=true",
  "package-win": "electron-packager . speedrun-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=views/assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"speedrun-app\"",
  "package-linux": "electron-packager . speedrun-app --overwrite --asar=true --platform=linux --arch=x64 --icon=views/assets/icons/png/1024x1024.png --prune=true --out=release-builds"
 },
 "author": "Iswenzz & Sheep Wizard & NitroFire",
 "license": "GPLv3",
 "dependencies": {
  "@fortawesome/fontawesome-free": "^5.12.0",
  "@fortawesome/fontawesome-svg-core": "^1.2.26",
  "@fortawesome/free-brands-svg-icons": "^5.12.0",
  "@fortawesome/free-regular-svg-icons": "^5.12.0",
  "@fortawesome/free-solid-svg-icons": "^5.12.0",
  "dialogs": "^2.0.1",
  "electron": "^2.0.18",
  "electron-packager": "^12.2.0",
  "execSync": "^1.0.2",
  "jquery": "^3.4.1",
  "opn": "^5.5.0",
  "particles.js": "^2.0.0",
  "ws": "^5.2.2",
  "xmlhttprequest": "^1.8.0"
 },
 "devDependencies": {
  "jest": "^25.1.0"
 },
 "jest": {
  "coverageReporters": [
   "text",
   "cobertura",
   "lcov"
  ],
  "collectCoverageFrom": [
   "**/src/**/*.{js,jsx,ts,tsx}",
   "!**/node_modules/**",
   "!**/src/utility/particles.js"
  ]
 }
}
